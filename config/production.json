{
  "Marathon": {
    "Endpoint": "http://192.168.1.155:8080"
  },

  "Bamboo": {
    "Endpoint": "http://192.168.1.127:8000",
    "Zookeeper": {
      "Host": "192.168.1.155:2181",
      "Path": "/bamboo",
      "ReportingDelay": 5
    }
  },

  "HAProxy": {
    "TemplatePath": "config/haproxy_template.cfg",
    "OutputPath": "/data/haproxy/haproxy.cfg",
    "ReloadCommand": "haproxy -f /data/haproxy/haproxy.cfg -p /var/run/haproxy.pid -D -sf $(cat /var/run/haproxy.pid)",
    "ReloadValidationCommand": "haproxy -c -f {{.}}"
  },

  "StatsD": {
    "Enabled": false,
    "Host": "localhost:8125",
    "Prefix": "bamboo-server.development."
  },

  "Application": {
  	"Id": "2048-zhguo"
  }
}
